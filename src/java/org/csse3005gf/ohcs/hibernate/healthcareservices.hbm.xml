<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="org.csse3005gf.ohcs.hibernate">
    <class name="HealthcareServices" table='"HEALTHCARESERVICES"'>
        <id name="healthcareid" type="java.lang.Integer">
            <column name='"HEALTHCAREID"' length="11" not-null="true" sql-type="int" />
            <generator class="sequence">
                <param name="sequence">HCSERVSEQ</param>
            </generator>
        </id>
    
   <!-- The first name is from java file, the second column is from db -->
    
        <property name="name" column='"NAME"' type="java.lang.String"></property>  
        <property name="phone" column='"PHONE"' type="java.lang.String"></property>
        <property name="servicetype" column='"SERVICETYPE"' type="java.lang.Integer"></property>
        <property name="locationid" column='"LOCATIONID"' type="java.lang.Integer"></property>
    
    </class>
  
</hibernate-mapping>
